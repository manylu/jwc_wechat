        </div>

        <div style="width: 100%;">
          <p class="informTitle"><a href="./getInform/<%- i %>"><%= informs[i].title %></a><p>
        </div>

      </div>
      <% } %>
    </div>
    
 </div>
    
  
  <script src='/javascripts/jquery-2.1.4.js'></script>
